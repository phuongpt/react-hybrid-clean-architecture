{
  "name": "nativeapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "preinstall": "cd ../core && yarn",
    "prestart": "yarn synccore && watchman watch-del-all",
